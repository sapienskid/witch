{
  "compilerOptions": {
    "baseUrl": ".",
    "inlineSourceMap": true,
    "inlineSources": true,
    "module": "ES2022",
    "target": "ES2022",
    "allowJs": true,
    "noImplicitAny": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "isolatedModules": true,
    "strictNullChecks": true,
  "lib": [
      "DOM",
      "ES2022"
  ],
  "esModuleInterop": true,
  "allowSyntheticDefaultImports": true
  },
  "include": [
    "**/*.ts"
  ]
}
